        "sequelize": "^6.37.7"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/cors": "^2.8.17",
        "@types/morgan": "^1.9.9",
        "@types/node": "^24.2.0",
        "rimraf": "^6.0.1",
        "ts-node-dev": "^2.0.0",
        "tsx": "^4.20.3",
        "typescript": "^5.9.2",
        "@dnd/typescript-config": "workspace:*"
    }
