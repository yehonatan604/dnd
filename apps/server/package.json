        "start:dev": "tsx src/index.ts",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "cors": "^2.8.5",
        "express": "^4.21.2",
        "morgan": "^1.10.1",
        "pg": "^8.16.3",
